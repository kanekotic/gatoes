{
  "name": "gatoo",
  "version": "0.1.113",
  "description": "google analytics export",
  "main": "lib/index.js",
  "repository": "git@github.com:kanekotic/gatoes.git",
  "author": "kanekotic <alvarojosepl@gmail.com>",
  "license": "MIT",
  "private": false,
  "bin": {
    "gatoo": "bin/gatoo.js"
  },
  "keywords": [
    "command line",
    "tool",
    "Google",
    "analytics",
    "ElasticSearch"
  ],
  "scripts": {
    "test": "jest",
    "test:dev": "jest --watchAll",
    "test:cov": "jest --coverage && codecov",
    "push": "yarn test && git push",
    "deploy:major": "yarn version --major",
    "deploy:minor": "yarn version --minor",
    "deploy:patch": "yarn version --patch",
    "deploy:push": "git push && git push --tags",
    "preversion": "yarn test"
  },
  "jest": {
    "automock": false,
    "testMatch": [
      "**/test/**/*-test.js"
    ],
    "testEnvironment": "node"
  },
  "devDependencies": {
    "codecov": "3.8.1",
    "coffee": "5.4.0",
    "faker": "5.1.0",
    "jest": "26.6.3"
  },
  "dependencies": {
    "commander": "6.2.0",
    "googleapis": "64.0.0",
    "moment": "2.29.1"
  }
}
